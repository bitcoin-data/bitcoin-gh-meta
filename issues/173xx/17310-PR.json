   "body" : "#include<iostream.h>\r\n#include<conio.h>\r\n\r\nmain()\r\n{float rupiah, poundsterling, yen, dollar; int pilihan;\r\ncout<<\"Konversi Mata Uang\"<<endl;\r\ncout<<\"1. Rupiah - Dollar \"<<endl;\r\ncout<<\"2. Rupiah - Poundsterling \"<<endl;\r\ncout<<\"3. Rupiah - Yen \"<<endl;\r\ncout<<\"\\nMasukkan pilihan anda = \";cin>>pilihan;\r\nif (pilihan==1)\r\n{cout<<\"\\n1. Rupiah - Dollar \"<<endl;\r\n cout<<\"Masukkan nilai rupiah = \";cin>>rupiah;\r\n dollar = rupiah/8000;\r\n cout<<\"Nilai dollar = \"<<dollar<<endl;}\r\n\r\nelse if(pilihan==2)\r\n{cout<<\"\\n2. Rupiah - Poundsterling \"<<endl;\r\n cout<<\"Masukkan nilai rupiah = \";cin>>rupiah;\r\n poundsterling = rupiah/10000;\r\n cout<<\"Nilai poundsterling = \"<<poundsterling<<endl;}\r\n\r\nelse if(pilihan==3)\r\n {cout<<\"\\n3. Rupiah - Yen \"<<endl;\r\n cout<<\"Masukkan nilai rupiah = \";cin>>rupiah;\r\n yen = rupiah/2000;\r\n cout<<\"Nilai yen = \"<<yen<<endl;}\r\nelse {cout<<\"\\nPilihan Tidak Tersedia\";}\r\n}",
