      "author_association" : "NONE",
      "body" : "My compile steps:\r\nsudo mkdir /data/.btc && cd /data/.btc\r\nsudo git clone https://github.com/bitcoin/bitcoin.git\r\ncd bitcoin\r\nsudo ./autogen.sh\r\nsudo ./configure\r\nsudo make\r\nsudo make install",
